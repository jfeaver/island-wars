server() {
  elm-go src/Main.elm -p 4444 --dir=./www -- --output=www/main.js
}

editor() {
  elm-go src/Editor.elm -p 4444 --dir=./www -- --output=www/main.js
}

fmt() {
  elm-format --yes src
}

build() {
  mkdir -p dist/
  cp www/* dist/
  elm make src/Editor.elm --output=dist/main.js --optimize
}
